  </PropertyGroup>
  <ItemGroup>
    <None Remove="Images\Add.png" />
    <None Remove="Images\Delete.png" />
    <None Remove="Images\FolderOpened.png" />
    <None Remove="Images\icon.ico" />
    <None Remove="Images\Save.png" />
