  </ItemGroup>
  <ItemGroup>
    <Content Include="Images\icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Fonts\Inter.ttf" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="data\games\take_a_nuke.mayonnaise_simulator\mods\hidden\" />
